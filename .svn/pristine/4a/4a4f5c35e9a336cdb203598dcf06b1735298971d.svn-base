<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>shaalakosh</groupId>
	<artifactId>shaalakosh-microservices</artifactId>
	<version>1.0</version>
	<packaging>pom</packaging>
	<parent>
		<groupId>org.springframework.cloud</groupId>
		<artifactId>spring-cloud-starter-parent</artifactId>
		<version>Brixton.RELEASE</version>
	</parent>
	<properties>
		<failOnMissingWebXml>false</failOnMissingWebXml>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<springframework.boot.version>1.5.1.RELEASE</springframework.boot.version>
		<spring.boot.version>2.0.0.RELEASE</spring.boot.version>
		<io.jsonwebtoken.version>0.6.0</io.jsonwebtoken.version>
		<jaxb.core.version>2.2.11</jaxb.core.version>
		<jaxb.impl.version>2.2.11</jaxb.impl.version>
		<xstream.version>1.4.7</xstream.version>
		<camel.xstream.version>2.21.0</camel.xstream.version>
		<logstash.logback.encoder.version>4.6</logstash.logback.encoder.version>
		<commons.dbcp2.version>2.1.1</commons.dbcp2.version>
		<mysql.connector.java.version>5.1.6</mysql.connector.java.version>
	</properties>
	<modules>
		<module>../account-service</module>
		<module>../authorization-service</module>
		<module>../discovery-service</module>
		<module>../gateway-service</module>
		<module>../student-service</module>
		<module>../school-service</module>
		<module>../master-service</module>
	</modules>
	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
</project>
